<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.irving.portfolioapp">

    <application
        android:name=".PortfolioApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".diceroller.DiceRollerScreen" />
        <activity android:name=".about.AboutScreen" />
        <activity android:name=".colormyviews.ColorMyViewsScreen" />
        <activity android:name=".trivia.TriviaScreen" />
        <activity android:name=".dessert.DessertScreen" />
        <activity android:name=".guest.GuestTheWorldActivity" />
    </application>

</manifest>
